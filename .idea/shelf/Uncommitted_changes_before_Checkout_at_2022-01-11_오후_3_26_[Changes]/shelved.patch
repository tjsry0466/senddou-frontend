+            <>
+                {display ? createElement(data) : <>
+                    <table>
+                        <tr>
+                            <th>학력: </th>
+                            &nbsp;
+                            <td>졸업</td>
+                        </tr>
+                        <tr>
+                            <th>년도: </th>
+                            &nbsp;
+                            <td>0000년</td>
+                        </tr>
+                        <tr>
+                            <th>학과: </th>
+                            &nbsp;
+                            <td>00과</td>
+                        </tr>
+                        <tr>
+                            <th>학점: </th>
+                            &nbsp;
+                            <td>0.0점</td>
+                        </tr>
+                    </table>
+                </>}
+            </>
+        </div>
+    );
+}
+
+Education4.defaultProps = {
+    data: [{
+        "title": "가로형 학력 항목",
+        "content": ["내용 입력"]
+    }],
+};
+
+export default Education4;
