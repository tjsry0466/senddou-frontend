+            (<div key={el.id}>
+                <table className='mt-6 text-1xl'>
+                    <tr>
+                        <th className='font-bold'>{el.title}: </th>
+                        &nbsp;
+                        <td>{createContent(el.content)}</td>
+                    </tr>
+                </table>
+            </div>),
+        );
+    }, [createContent]);
+
+    return (
+        data && <div className='h-full'>
+            <div className={`${display && 'text-4xl'} font-bold p-2 text-center bg-blue-200`}>학력사항</div>
